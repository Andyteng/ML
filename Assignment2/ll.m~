function [ll] = ll(X)
%  Useful info
labels = getlab(X);
X = getdata(X);
[N,D] = size(X);
L0 = labels(labels == -1);
X0 = X(labels == -1,:);
N0 = size(X0,1);
L1 = labels(labels == 1);
X1 = X(labels == 1);
N1 = size(X1,1);

prior0 = N0/N0+N1;
prior1 = N1/N0+N1;

labeled_mean0 = mean(X0,1);
labeled_mean1 = mean(X1,1);
mean0 = labeled_mean0;
mean1 = labeled_mean1; 
end 